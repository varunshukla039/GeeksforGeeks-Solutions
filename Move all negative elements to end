class Solution:
    def segregateElements(self, arr, n):
        # Your code goes here
        arr1=[]
        arr2=[]
        for i in arr:
            if i<0:
                arr1.append(i)
                
            else:
                arr2.append(i)
        
        arr3=arr2+arr1
        for i in range(n):
            arr[i]=arr3[i]
